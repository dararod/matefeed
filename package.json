{
  "name": "@matefeed/matefeed",
  "version": "0.1.0",
  "main": "server.ts",
  "repository": "https://github.com/EstebanBorai/fastify-next-project.git",
  "author": "Esteban Borai <estebanborai@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "NEXT_TELEMETRY_DISABLED=1 yarn build:client && yarn build:server",
    "build:production": "yarn run build && ./bin/prepare_prod",
    "build:client": "NEXT_TELEMETRY_DISABLED=1 next build ./src/client",
    "build:server": "tsc",
    "check:server": "tsc --noEmit",
    "dev": "NEXT_TELEMETRY_DISABLED=1 ts-node-dev --ignore-watch ./src/client --transpile-only ./src/main.ts",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "fmt": "pnpm run eslint && pnpm run prettier",
    "knex": "knex --knexfile src/config/database/knexfile.ts",
    "prettier": "prettier --write -c \"**/*.{md,json,yml,sh}\"",
    "start": "NEXT_TELEMETRY_DISABLED=1 node ./dist/main.js",
    "stylelint": "stylelint \"src/client/**/*.css\""
  },
  "dependencies": {
    "@sinclair/typebox": "^0.23.2",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "classnames": "^2.3.1",
    "fastify": "^3.25.3",
    "fastify-cookie": "^5.5.0",
    "fastify-jwt": "^4.1.3",
    "fastify-plugin": "^3.0.0",
    "fastify-swagger": "^4.13.1",
    "formik": "^2.2.9",
    "graphql": "^16.2.0",
    "http-status-codes": "^2.2.0",
    "js-cookie": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.15",
    "knex-stringcase": "^1.4.6",
    "mercurius": "^9.3.5",
    "mercurius-codegen": "^3.3.0",
    "ms": "^2.1.3",
    "next": "^12.0.7",
    "pg": "^8.7.1",
    "pino-pretty": "^7.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "urql": "^2.0.6",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/js-cookie": "^3.0.1",
    "@types/jsonwebtoken": "^8.5.7",
    "@types/ms": "^0.7.31",
    "@types/node": "^17.0.18",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.11",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.8.1",
    "altair-fastify-plugin": "^4.4.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.5.0",
    "prettier": "^2.5.1",
    "stylelint": "^14.2.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  }
}
